import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		while(T-->0){
		    
		    int A = sc.nextInt();
		    
		    // if the number is even then it does have remainder when divided by 2
		    // and same as this for multiple of 7
		    if(A%2 == 0 && A%7 == 0){
		        System.out.println("ALICE");
		        
		        // if the number id odd is gave a remainder when the number divided by 2
		        // and does not when divided by 9 for this condition
		    }else if(A%2 != 0 && A%9 == 0){
		        System.out.println("BOB");
		        
		        // if the above condition do not ure then print charlie
		    }else{
		        System.out.println("CHARLIE");
		    }
		}
	}
}




