#include <iostream>
#include<bits/stdc++.h>
#define ll long long
using namespace std;

int main() 
{
	ll t;
	cin>>t;
	while(t--)
	{
	    ll x,y;
	    cin>>x>>y;
	    if(y%5==0)
	    y=y/5;
	    else
	    y=y/5+1;
	    if(x>=y)
	    cout<<"YES"<<endl;
	    else
	    cout<<"NO"<<endl;
	}
	return 0;
}
