#include <iostream>
using namespace std;

bool check (int x,int y,int z){
    if((x+y)<=z){
    return true;
    }
    return false;
}
int main() {
     int t;
     cin>>t;
     while(t--){
         int x,y,z;
         cin>>x>>y>>z;
         check(x,y,z) ? cout<<"YES"<<endl : cout<<"NO"<<endl;
     }
	return 0;
}
