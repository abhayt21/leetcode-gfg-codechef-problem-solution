import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner kb = new Scanner(System.in) ;
		int t = kb.nextInt();
		while(t--!=0)
		{
		    
		    int n = kb.nextInt() ;
		    int m = kb.nextInt() ;
		   
		    if(m * 36 >= n )
		    {
		        System.out.println("YES") ;
		    }
		    else {
		        System.out.println("NO") ;
		        
		    }
		    
		    
		}
		
		
	}
}
