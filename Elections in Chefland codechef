#include <iostream>
using namespace std;

int main() {
	// your code goes here
	 int t ;
	 cin>> t ;
	 while(t--!=0)
	 {
	     int n , x  , p  , count = 0;
	     cin>> n >> x ;
	      while(n--!=0){
	          cin>> p  ;
	        
	       if( p >= x )
	         count++ ;
	            
	      }
	     cout<<count <<endl ; 
	 }
	return 0;
}
